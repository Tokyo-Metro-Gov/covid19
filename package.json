{
  "name": "covid19",
  "version": "1.0.0",
  "description": "東京都 新型コロナウイルス感染症対策サイト",
  "author": "東京都",
  "private": true,
  "node-options": {
    "experimental-json-modules": true,
    "heapsnapshot-near-heap-limit": 3,
    "max-old-space-size": 2048
  },
  "engines": {
    "node": ">=16.13.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=node-options NODE_VERSION= GENERATE_ENV=development nuxt",
    "dev-no-axe": "cross-env NODE_ENV=dev-no-axe NODE_OPTIONS=node-options NODE_VERSION= GENERATE_ENV=development nuxt",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production NODE_VERSION= nuxt",
    "generate:deploy": "cross-env GENERATE_ENV=production NODE_OPTIONS=node-options NODE_VERSION= nuxt build --fail-on-page-error && yarn functions:build",
    "generate:dev": "cross-env GENERATE_ENV=development NODE_OPTIONS=node-options NODE_VERSION= nuxt build --fail-on-page-error && yarn functions:build",
    "generate": "eslint \"./**/*.{js,jsx,ts,tsx,vue}\" && nuxt build",
    "test": "echo 'skip tests (there is no test)'",
    "lint": "eslint \"./**/*.{js,jsx,ts,tsx,vue}\"",
    "lint:fix": "eslint \"./**/*.{js,jsx,ts,tsx,vue}\" --fix",
    "stylelint": "stylelint '**/*.{css,scss,sass,vue}'",
    "stylelint:fix": "stylelint --fix '**/*.{css,scss,sass,vue}'",
    "clean-hardsource": "rimraf ./node_modules/.cache/hard-source",
    "functions:dev": "netlify-lambda serve functions",
    "functions:build": "netlify-lambda build functions",
    "generate-data-converters": "ts-node --compiler-options='{\"module\":\"commonjs\"}' ./tool/generateDataConverters.ts",
    "prepare": "husky install",
    "danger:ci": "danger ci"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss,sass,vue}": "stylelint"
  },
  "resolutions": {},
  "dependencies": {
    "@nuxt/types": "2.15.8",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/gtm": "2.4.0",
    "@nuxtjs/i18n": "7.2.0",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/router": "1.6.1",
    "axe-core": "4.3.5",
    "chart.js": "2.9.4",
    "netlify-lambda": "2.0.15",
    "nuxt": "2.15.8",
    "vue": "2.6.14"
  },
  "devDependencies": {
    "@babel/core": "7.16.0",
    "@babel/plugin-proposal-private-methods": "^7.16.0",
    "@babel/runtime-corejs3": "7.16.3",
    "@mdi/js": "6.5.95",
    "@nuxt/postcss8": "1.1.3",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/vuetify": "1.12.1",
    "@types/chart.js": "2.9.34",
    "@types/lodash": "4.14.177",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.3.1",
    "@typescript-eslint/experimental-utils": "5.3.1",
    "@typescript-eslint/parser": "5.3.1",
    "@typescript-eslint/scope-manager": "5.3.1",
    "@typescript-eslint/types": "5.3.1",
    "@typescript-eslint/typescript-estree": "5.3.1",
    "@vue/compiler-sfc": "3.2.22",
    "core-js": "3.19.1",
    "cross-env": "7.0.3",
    "danger": "10.7.1",
    "dayjs": "1.10.7",
    "eslint": "8.2.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-import": "2.25.3",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "eslint-plugin-unicorn": "38.0.1",
    "eslint-plugin-vue": "8.0.3",
    "eslint-scope": "6.0.0",
    "eslint-utils": "3.0.0",
    "fork-ts-checker-webpack-plugin": "6.4.0",
    "husky": "7.0.4",
    "lint-staged": "11.2.6",
    "lodash": "4.17.21",
    "nuxt-property-decorator": "2.9.1",
    "nuxt-svg-loader": "1.2.0",
    "nuxt-webfontloader": "1.1.0",
    "postcss-html": "1.2.0",
    "postcss-import": "14.0.2",
    "postcss-scss": "4.0.2",
    "prettier": "2.4.1",
    "quicktype": "15.0.260",
    "sass": "1.43.4",
    "sass-loader": "10.2.0",
    "stylelint": "14.0.1",
    "stylelint-config-html": "1.0.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended-scss": "5.0.1",
    "stylelint-config-recommended-vue": "1.0.0",
    "stylelint-config-standard": "23.0.0",
    "stylelint-prettier": "2.0.0",
    "stylelint-scss": "4.0.0",
    "ts-loader": "8.3.0",
    "ts-node": "10.4.0",
    "typescript": "4.4.4",
    "vue-axe": "3.1.2",
    "vue-chartjs": "3.5.1",
    "vue-meta": "2.4.0",
    "vue-scrollto": "2.20.0",
    "vue-spinner": "1.0.4",
    "vue-template-compiler": "2.6.14"
  }
}
