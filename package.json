{
  "name": "covid19",
  "version": "1.0.0",
  "description": "東京都 新型コロナウイルス感染症対策サイト",
  "author": "東京都",
  "private": true,
  "engines": {
    "node": "^14.16.0 || ^16.11.1"
  },
  "node-options": {
    "experimental-json-modules": true,
    "pending-deprecation": 0,
    "heapsnapshot-near-heap-limit": 3,
    "max-old-space-size": 2048
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development NODE_OPTIONS=node-options NODE_NO_WARNINGS=1 GENERATE_ENV=development nuxt dev",
    "dev-no-axe": "cross-env NODE_ENV=dev-no-axe NODE_OPTIONS=node-options NODE_NO_WARNINGS=1 GENERATE_ENV=development nuxt dev",
    "build": "nuxt build",
    "start": "cross-env NODE_ENV=production nuxt",
    "generate:deploy": "cross-env GENERATE_ENV=production NODE_OPTIONS=node-options NODE_NO_WARNINGS=1 nuxt generate --fail-on-page-error && yarn functions:build",
    "generate:dev": "cross-env GENERATE_ENV=development NODE_OPTIONS=node-options NODE_NO_WARNINGS=1 nuxt generate --fail-on-page-error && yarn functions:build",
    "generate": "eslint \"./**/*.{js,jsx,ts,tsx,vue}\" && nuxt build",
    "test": "echo 'skip tests (there is no test)'",
    "lint": "eslint \"./**/*.{js,jsx,ts,tsx,vue}\"",
    "lint:fix": "eslint \"./**/*.{js,jsx,ts,tsx,vue}\" --fix",
    "stylelint": "stylelint '**/*.{css,scss,sass,vue}'",
    "stylelint:fix": "stylelint --fix '**/*.{css,scss,sass,vue}'",
    "clean-hardsource": "rimraf ./node_modules/.cache/hard-source",
    "functions:dev": "netlify-lambda serve functions",
    "functions:build": "netlify-lambda build functions",
    "generate-data-converters": "ts-node --compiler-options='{\"module\":\"commonjs\"}' ./tool/generateDataConverters.ts",
    "prepare": "husky install",
    "danger:ci": "danger ci"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss,sass,vue}": "stylelint"
  },
  "resolutions": {
    "@nuxtjs/vuetify/**/sass": "^1.32.13",
    "node/eslint": "^7.32.0",
    "node/@typescript-eslint/eslint-plugin": "^4.33.0",
    "node/@typescript-eslint/experimental-utils": "^4.33.0",
    "node/@typescript-eslint/parser": "^4.33.0"
  },
  "dependencies": {
    "@nuxtjs/router": "1.6.1",
    "babel-plugin-transform-scss": "1.1.0",
    "chart.js": "2.9.4",
    "netlify-lambda": "2.0.15",
    "nuxt": "2.15.8",
    "vue": "2.6.14",
    "vuetify": "2.5.10"
  },
  "devDependencies": {
    "@babel/core": "7.15.8",
    "@mdi/js": "6.3.95",
    "@nuxt/types": "2.15.8",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/eslint-config": "6.0.1",
    "@nuxtjs/eslint-config-typescript": "6.0.1",
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/gtm": "2.4.0",
    "@nuxtjs/i18n": "7.1.0",
    "@nuxtjs/pwa": "3.3.5",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/svg": "0.3.0",
    "@nuxtjs/vuetify": "1.12.1",
    "@types/chart.js": "2.9.34",
    "@types/lodash": "4.14.176",
    "@types/node": "14.17.28",
    "@types/regenerator-runtime": "0.13.1",
    "@typescript-eslint/eslint-plugin": "4.33.0",
    "@typescript-eslint/experimental-utils": "4.33.0",
    "@typescript-eslint/parser": "4.33.0",
    "@vue/compiler-sfc": "3.2.20",
    "axe-core": "4.3.3",
    "core-js": "3.18.3",
    "cross-env": "7.0.3",
    "danger": "10.7.0",
    "dayjs": "1.10.7",
    "eslint": "7.32.0",
    "eslint-config-prettier": "7.2.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "25.2.2",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-promise": "5.1.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "eslint-plugin-unicorn": "37.0.1",
    "eslint-plugin-vue": "7.20.0",
    "husky": "7.0.4",
    "lint-staged": "11.2.3",
    "lodash": "4.17.21",
    "nuxt-property-decorator": "2.9.1",
    "nuxt-webfontloader": "1.1.0",
    "postcss-html": "1.0.1",
    "postcss-sass": "0.5.0",
    "postcss-scss": "4.0.1",
    "prettier": "2.4.1",
    "quicktype": "15.0.260",
    "regenerator-runtime": "0.13.9",
    "sass": "1.43.3",
    "sass-loader": "10.2.0",
    "stylelint": "14.0.0",
    "stylelint-config-sass-guidelines": "8.0.0",
    "stylelint-config-standard": "23.0.0",
    "stylelint-prettier": "1.2.0",
    "stylelint-scss": "4.0.0",
    "stylelint-webpack-plugin": "3.0.1",
    "ts-loader": "8.3.0",
    "ts-node": "10.4.0",
    "typescript": "4.4.4",
    "vue-axe": "3.1.2",
    "vue-chartjs": "3.5.1",
    "vue-meta": "2.4.0",
    "vue-scrollto": "2.20.0",
    "vue-spinner": "1.0.4",
    "vue-template-compiler": "2.6.14"
  }
}
