{
  "name": "covid19",
  "version": "1.0.0",
  "description": "東京都 新型コロナウイルス感染症対策サイト",
  "author": "東京都",
  "private": true,
  "engines": {
    "node": ">=10.23.2"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development GENERATE_ENV=development nuxt-ts",
    "dev-no-axe": "cross-env NODE_ENV=dev-no-axe GENERATE_ENV=development nuxt-ts",
    "build": "nuxt-ts build",
    "start": "cross-env NODE_ENV=production nuxt-ts start",
    "generate:deploy": "cross-env GENERATE_ENV=production NODE_OPTIONS=--max_old_space_size=2048 nuxt-ts generate --fail-on-page-error && yarn --ignore-engines --ignore-platform --ignore-optional functions:build",
    "generate:dev": "cross-env GENERATE_ENV=development NODE_OPTIONS=--max_old_space_size=2048 nuxt-ts generate --fail-on-page-error && yarn --ignore-engines --ignore-platform --ignore-optional functions:build",
    "generate": "eslint \"./**/*.{js,ts,vue}\" && nuxt-ts generate",
    "test": "echo 'skip tests (there is no test)'",
    "lint": "eslint \"./**/*.{js,ts,vue}\"",
    "lint:fix": "eslint \"./**/*.{js,ts,vue}\" --fix",
    "stylelint": "stylelint '**/*.{css,scss,vue}'",
    "stylelint:fix": "stylelint --fix '**/*.{css,scss,vue}'",
    "clean-hardsource": "rimraf ./node_modules/.cache/hard-source",
    "functions:dev": "netlify-lambda serve functions",
    "functions:build": "netlify-lambda build functions"
  },
  "lint-staged": {
    "*.{js,ts,css,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "./pre-commit.sh && lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/types": "2.14.12",
    "@nuxt/typescript-runtime": "2.0.2",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/gtm": "2.4.0",
    "@nuxtjs/pwa": "3.3.5",
    "axe-core": "4.1.2",
    "chart.js": "2.9.4",
    "cross-env": "7.0.3",
    "dayjs": "1.10.4",
    "netlify-lambda": "2.0.3",
    "nuxt": "2.14.12",
    "nuxt-i18n": "6.20.3",
    "vue": "2.6.12",
    "vue-chartjs": "3.5.1",
    "vue-meta": "2.4.0",
    "vue-scrollto": "2.20.0",
    "vue-spinner": "1.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.13.8",
    "@babel/helper-define-map": "7.12.13",
    "@babel/preset-env": "7.12.17",
    "@babel/runtime-corejs3": "7.13.8",
    "@mdi/js": "5.9.55",
    "@nuxt/typescript-build": "2.0.6",
    "@nuxtjs/eslint-config": "5.0.0",
    "@nuxtjs/eslint-config-typescript": "5.0.0",
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/stylelint-module": "4.0.0",
    "@nuxtjs/vuetify": "1.11.3",
    "@types/chart.js": "2.9.30",
    "core-js": "3.9.0",
    "eslint": "7.20.0",
    "eslint-config-prettier": "6.15.0",
    "eslint-plugin-prettier": "3.3.1",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tsdoc": "0.2.11",
    "husky": "5.1.1",
    "lint-staged": "10.5.4",
    "nuxt-property-decorator": "2.9.1",
    "nuxt-purgecss": "1.0.0",
    "nuxt-svg-loader": "1.2.0",
    "nuxt-webfontloader": "1.1.0",
    "prettier": "2.2.1",
    "rimraf": "3.0.2",
    "stylelint": "13.11.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-scss": "3.19.0",
    "ts-loader": "8.0.17",
    "typescript": "4.2.2",
    "vue-axe": "2.4.4",
    "vue-template-compiler": "2.6.12"
  }
}
