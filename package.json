{
  "name": "covid19",
  "version": "1.0.0",
  "description": "東京都 新型コロナウイルス感染症対策サイト",
  "author": "東京都",
  "private": true,
  "engines": {
    "node": ">=16.13.0"
  },
  "node-options": {
    "experimental-json-modules": true,
    "heapsnapshot-near-heap-limit": 3,
    "max-old-space-size": 4096,
    "fail-on-error": true
  },
  "scripts": {
    "dev": "cross-env -C=development NODE_OPTIONS=--max_old_space_size=2048 nuxt dev",
    "dev-no-axe": "cross-env -C=dev-no-axe NODE_OPTIONS=--max_old_space_size=2048 nuxt dev",
    "build": "nuxt generate",
    "start": "cross-env -C=production nuxt start",
    "generate:deploy": "cross-env -C=production NODE_OPTIONS=--max_old_space_size=2048 yarn build --fail-on-page-error && yarn functions:build",
    "generate:dev": "cross-env -C=development NODE_OPTIONS=--max_old_space_size=2048 yarn build --fail-on-page-error && yarn functions:build",
    "generate": "eslint \"./**/*.{js,jsx,cjs,mjs,ts,tsx,vue}\" && yarn build",
    "test": "echo 'skip tests (there is no test)'",
    "lint": "eslint \"./**/*.{js,jsx,cjs,mjs,ts,tsx,vue}\"",
    "lint:fix": "eslint \"./**/*.{js,jsx,cjs,mjs,ts,tsx,vue}\" --fix",
    "stylelint": "stylelint '**/*.{css,scss,sass,vue}'",
    "stylelint:fix": "stylelint --fix '**/*.{css,scss,sass,vue}'",
    "clean-hardsource": "rimraf ./node_modules/.cache/hard-source",
    "functions:dev": "netlify-lambda serve functions",
    "functions:build": "netlify-lambda build functions",
    "generate-data-converters": "ts-node --compiler-options='{\"module\":\"commonjs\"}' ./tool/generateDataConverters.ts",
    "prepare": "husky install",
    "danger:ci": "danger ci"
  },
  "lint-staged": {
    "*.{js,jsx,cjs,mjs,ts,tsx,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss,sass,vue}": "stylelint"
  },
  "dependencies": {
    "@nuxt/types": "2.15.8",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/gtm": "2.4.0",
    "@nuxtjs/i18n": "7.2.0",
    "axe-core": "4.3.5",
    "chart.js": "2.9.4",
    "cross-env": "7.0.3",
    "dayjs": "1.10.7",
    "netlify-lambda": "2.0.15",
    "nuxt": "2.15.8",
    "vue": "2.6.14",
    "vue-chartjs": "3.5.1",
    "vue-meta": "2.4.0",
    "vue-scrollto": "2.20.0",
    "vue-spinner": "1.0.4",
    "workbox-webpack-plugin": "6.4.2"
  },
  "devDependencies": {
    "@babel/core": "7.16.5",
    "@babel/runtime-corejs3": "7.16.5",
    "@mdi/js": "6.5.95",
    "@nuxt/postcss8": "1.1.3",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/eslint-config-typescript": "8.0.0",
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/vuetify": "1.12.3",
    "@types/chart.js": "2.9.35",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.17",
    "core-js": "3.20.0",
    "danger": "10.7.1",
    "eslint": "8.5.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "husky": "7.0.4",
    "lint-staged": "12.1.3",
    "lodash": "4.17.21",
    "nuxt-property-decorator": "2.9.1",
    "nuxt-purgecss": "1.0.0",
    "nuxt-svg-loader": "1.2.0",
    "nuxt-webfontloader": "1.1.0",
    "postcss": "8.4.5",
    "postcss-html": "1.3.0",
    "postcss-scss": "3.0.5",
    "prettier": "2.5.1",
    "quicktype": "15.0.260",
    "sass": "1.45.1",
    "sass-loader": "10.2.0",
    "source-map": "0.7.3",
    "source-map-js": "1.0.1",
    "stylelint": "14.1.0",
    "stylelint-config-html": "1.0.0",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-recommended-scss": "5.0.2",
    "stylelint-config-standard-scss": "3.0.0",
    "ts-loader": "8.3.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.4",
    "vue-axe": "2.4.4",
    "vue-template-compiler": "2.6.14"
  }
}
