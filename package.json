{
  "name": "covid19",
  "version": "1.0.0",
  "description": "東京都 新型コロナウイルス感染症対策サイト",
  "author": "東京都",
  "private": true,
  "engines": {
    "node": ">=16.13.0"
  },
  "node-options": {
    "max-old-space-size": 2048,
    "v8-pool-size": 16
  },
  "scripts": {
    "dev": "cross-env -C=development UV_THREADPOOL_SIZE=16 DISABLE_V8_COMPILE_CACHE=1 nuxt dev",
    "dev-no-axe": "cross-env -C=dev-no-axe UV_THREADPOOL_SIZE=16 DISABLE_V8_COMPILE_CACHE=1 NODE_OPTIONS=node-options nuxt dev",
    "build": "nuxt generate",
    "start": "cross-env -C=production nuxt start",
    "generate:deploy": "cross-env -C=productio UV_THREADPOOL_SIZE=16n DISABLE_V8_COMPILE_CACHE=1 NODE_OPTIONS=node-options yarn build && yarn functions:build",
    "generate:dev": "cross-env -C=development UV_THREADPOOL_SIZE=16 DISABLE_V8_COMPILE_CACHE=1 NODE_OPTIONS=node-options yarn build && yarn functions:build",
    "generate": "eslint \"./**/*.{js,jsx,cjs,mjs,ts,tsx,vue}\" && yarn build",
    "test": "echo 'skip tests (there is no test)'",
    "lint": "eslint \"./**/*.{js,jsx,cjs,mjs,ts,tsx,vue}\"",
    "lint:fix": "eslint \"./**/*.{js,jsx,cjs,mjs,ts,tsx,vue}\" --fix",
    "stylelint": "stylelint '**/*.{css,scss,sass,vue}'",
    "stylelint:fix": "stylelint --fix '**/*.{css,scss,sass,vue}'",
    "clean-hardsource": "rimraf ./node_modules/.cache/hard-source",
    "functions:dev": "netlify-lambda serve functions",
    "functions:build": "netlify-lambda build functions",
    "generate-data-converters": "ts-node --compiler-options='{\"module\":\"commonjs\"}' ./tool/generateDataConverters.ts",
    "prepare": "husky install",
    "danger:ci": "danger ci"
  },
  "lint-staged": {
    "*.{js,ts,css,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss,vue}": "stylelint"
  },
  "resolutions": {
    "@nuxtjs/vuetify/**/sass": "1.32.13"
  },
  "dependencies": {
    "@babel/runtime-corejs3": "7.17.2",
    "@nuxt/types": "2.15.8",
    "@nuxtjs/dotenv": "1.4.1",
    "@nuxtjs/gtm": "2.4.0",
    "@nuxtjs/i18n": "7.2.0",
    "@nuxtjs/pwa": "3.3.5",
    "axe-core": "4.4.1",
    "chart.js": "2.9.4",
    "cross-env": "7.0.3",
    "dayjs": "1.10.7",
    "netlify-lambda": "2.0.15",
    "nuxt": "2.15.8",
    "vue": "2.6.14",
    "vue-chartjs": "3.5.1",
    "vue-meta": "2.4.0",
    "vue-scrollto": "2.20.0",
    "vue-spinner": "1.0.4"
  },
  "devDependencies": {
    "@babel/core": "7.17.2",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "7.16.7",
    "@babel/plugin-proposal-partial-application": "7.16.7",
    "@mdi/js": "6.5.95",
    "@nuxt/postcss8": "1.1.3",
    "@nuxt/typescript-build": "2.1.0",
    "@nuxtjs/babel-preset-app": "0.8.0",
    "@nuxtjs/eslint-config-typescript": "8.0.0",
    "@nuxtjs/google-analytics": "2.4.0",
    "@nuxtjs/stylelint-module": "4.1.0",
    "@nuxtjs/vuetify": "1.12.3",
    "@types/chart.js": "2.9.35",
    "@types/lodash": "4.14.178",
    "@types/node": "16.11.21",
    "@vue/compiler-dom": "3.2.30",
    "babel-loader": "8.2.3",
    "babel-plugin-transform-scss": "1.1.0",
    "babel-plugin-tsconfig-paths": "1.0.2",
    "babel-preset-typescript-vue": "1.1.1",
    "core-js": "3.20.3",
    "css-loader": "5.2.7",
    "danger": "10.8.0",
    "eslint": "8.8.0",
    "eslint-config-prettier": "8.3.0",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "eslint-plugin-tsdoc": "0.2.14",
    "husky": "7.0.4",
    "lint-staged": "12.3.2",
    "lodash": "4.17.21",
    "nuxt-property-decorator": "2.9.1",
    "nuxt-purgecss": "1.0.0",
    "nuxt-svg-loader": "1.2.0",
    "nuxt-webfontloader": "1.1.0",
    "package.json": "^2.0.1",
    "postcss": "8.4.6",
    "postcss-loader": "5.3.0",
    "prettier": "2.5.1",
    "quicktype": "15.0.260",
    "sass": "1.49.7",
    "sass-loader": "7.3.1",
    "sass-resources-loader": "2.2.4",
    "style-loader": "3.3.1",
    "stylelint": "13.13.1",
    "stylelint-config-prettier": "9.0.3",
    "stylelint-config-standard": "22.0.0",
    "stylelint-scss": "3.21.0",
    "ts-loader": "8.3.0",
    "ts-node": "10.4.0",
    "typescript": "4.5.5",
    "vue-axe": "2.4.4",
    "vue-loader": "15.9.8",
    "vue-style-loader": "4.1.3",
    "vue-template-compiler": "2.6.14",
    "vuetify-loader": "1.7.3",
    "yarn.lock": "^0.0.1-security"
  }
}
