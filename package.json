{
  "name": "covid19",
  "version": "1.0.0",
  "description": "東京都 新型コロナウイルス感染症対策サイト",
  "author": "東京都",
  "private": true,
  "engines": {
    "node": ">=10.19.0"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=development nuxt-ts",
    "build": "nuxt-ts build",
    "start": "cross-env NODE_ENV=production nuxt-ts start",
    "generate:deploy": "cross-env NODE_ENV=production nuxt-ts generate",
    "generate:dev": "cross-env NODE_ENV=development nuxt-ts generate",
    "generate": "eslint './**/*.{js,ts,vue}' && nuxt-ts generate",
    "lint-and-generate": "eslint './**/*.{js,ts,vue}' && nuxt-ts generate",
    "test": "echo 'skip tests (there is no test)'",
    "lint": "eslint './**/*.{js,ts,vue}'",
    "lint:fix": "eslint './**/*.{js,ts,vue}' --fix",
    "clean-hardsource": "rimraf ./node_modules/.cache/hard-source"
  },
  "lint-staged": {
    "*.{js,ts,css,vue}": [
      "eslint --fix"
    ],
    "*.{css,scss,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.0",
    "@nuxtjs/axios": "^5.3.6",
    "@nuxtjs/dotenv": "^1.4.0",
    "@nuxtjs/pwa": "^3.0.0-0",
    "chart.js": "^2.9.3",
    "cross-env": "^5.2.0",
    "dayjs": "^1.8.21",
    "express": "^4.16.4",
    "nuxt": "^2.11.0",
    "nuxt-i18n": "^6.6.0",
    "vue-chartjs": "^3.5.0",
    "vue-property-decorator": "^8.4.0",
    "vue-scrollto": "^2.17.1",
    "vue-spinner": "^1.0.3"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "^2.1.0",
    "@mdi/font": "^5.0.45",
    "@nuxt/typescript-build": "^0.6.0",
    "@nuxtjs/eslint-config-typescript": "^1.0.2",
    "@nuxtjs/google-analytics": "^2.2.3",
    "@nuxtjs/stylelint-module": "^3.2.2",
    "@nuxtjs/vuetify": "^1.0.0",
    "@types/chart.js": "^2.9.14",
    "@vue/test-utils": "^1.0.0-beta.27",
    "babel-jest": "^24.1.0",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-tsdoc": "^0.2.3",
    "husky": "^4.2.3",
    "jest": "^24.1.0",
    "lint-staged": "^10.0.8",
    "nodemon": "^1.18.9",
    "nuxt-purgecss": "^0.2.1",
    "nuxt-svg-loader": "^1.2.0",
    "prettier": "^1.19.1",
    "stylelint": "^13.2.0",
    "rimraf": "^3.0.2",
    "vue-axe": "^1.0.8",
    "vue-jest": "^4.0.0-0"
  }
}
